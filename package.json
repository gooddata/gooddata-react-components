{
  "name": "@gooddata/react-components",
  "version": "2.5.0",
  "description": "GoodData React Components",
  "main": "dist/index.js",
  "repository": "git@github.com:gooddata/gooddata-component-sdk.git",
  "author": "GoodData",
  "license": "SEE LICENSE IN LICENSE.txt",
  "files": [
    "dist",
    "src",
    "styles"
  ],
  "devDependencies": {
    "@gooddata/js-utils": "0.8.0",
    "@gooddata/mock-js": "0.0.26",
    "@storybook/addon-options": "3.2.6",
    "@storybook/react": "3.2.6",
    "@types/jest": "20.0.4",
    "@types/lodash": "4.14.61",
    "@types/node": "7.0.8",
    "@types/react": "16.0.4",
    "@types/react-addons-test-utils": "0.14.17",
    "@types/react-dom": "15.5.4",
    "enzyme": "2.8.0",
    "jest": "21.0.0",
    "jest-junit": "3.0.0",
    "node-sass": "4.5.3",
    "node-sass-magic-importer": "4.1.5",
    "react": "15.3.2",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "5.0.6",
    "sass-loader": "6.0.6",
    "ts-jest": "21.0.0",
    "ts-loader": "2.3.3",
    "tslint": "5.4.3",
    "tslint-config-gooddata": "0.0.2",
    "typescript": "2.4.2"
  },
  "dependencies": {
    "@gooddata/data-layer": "3.2.0",
    "@gooddata/goodstrap": "53.0.1",
    "@gooddata/indigo-visualizations": "0.12.0-tomasreichmann-TRE-RAIL-289-Enable-support-for-width-and-height-properties-on-Visualization-component-2017-09-25T14-00-17-026Z",
    "classnames": "2.2.5",
    "gooddata": "4.0.0",
    "lodash": "4.17.4",
    "numeral": "2.0.6",
    "react-intl": "2.2.3",
    "rxjs": "5.4.3",
    "uuid": "3.0.1"
  },
  "peerDependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "scripts": {
    "test": "jest --watch",
    "build-css": "node-sass --importer node_modules/node-sass-magic-importer/dist/cli.js -o styles/css styles/scss/main.scss",
    "dev": "rm -rf dist && tsc -d --outDir dist --watch && cp -rf src/translations/ dist/translations/",
    "prepublish": "rm -rf dist && tsc -d --outDir dist && cp -rf src/translations/ dist/translations/ && yarn build-css",
    "tslint": "tslint --format verbose --config tslint.json '{src,stories}/**/*.{ts,tsx}'",
    "validate": "rm -rf dist && tsc -p tsconfig.base.json --noEmit && yarn tslint",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o dist-storybook"
  },
  "typings": "dist/index.d.ts",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?)$",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "collectCoverage": false,
    "mapCoverage": true,
    "coverageReporters": [
      "html"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
